<xs:schema elementFormDefault='qualified' targetNamespace='http://fasb.org/basi/stm-std/2012-01-31' xmlns:link='http://www.xbrl.org/2003/linkbase' xmlns:stm-std-basi='http://fasb.org/basi/stm-std/2012-01-31' xmlns:xlink='http://www.w3.org/1999/xlink' xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
