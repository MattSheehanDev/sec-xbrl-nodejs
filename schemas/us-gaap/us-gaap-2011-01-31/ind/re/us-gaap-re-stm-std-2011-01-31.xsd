<xs:schema elementFormDefault='qualified' targetNamespace='http://fasb.org/re/stm-std/2011-01-31' xmlns:link='http://www.xbrl.org/2003/linkbase' xmlns:stm-std-re='http://fasb.org/re/stm-std/2011-01-31' xmlns:xbrli='http://www.xbrl.org/2003/instance' xmlns:xlink='http://www.w3.org/1999/xlink' xmlns:xs='http://www.w3.org/2001/XMLSchema'>
