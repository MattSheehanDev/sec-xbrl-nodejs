{
    "compilerOptions": {
        "target": "es6",
        "sourceMap": true,
        "declaration": true,
        "module": "commonjs",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": true,
        "outDir": "./bin"
    },
    "exclude": [
        "node_modules",
        "bin",
        ".vscode",
        "typings/**",
        "gulpfile.ts"
    ]
}
